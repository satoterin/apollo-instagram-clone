{
   "name": "server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "main": "dist/index.js",
   "scripts": {
      "start": "node dist/index.js",
      "watch": "tsc -w",
      "server": "nodemon dist/index.js",
      "server2": "TS_NODE=true nodemon --exec ts-node src/index.ts",
      "redis": "redis-server",
      "dev": "concurrently \"yarn watch\" \"yarn redis\" \"yarn server\" ",
      "dev2": "concurrently \"yarn watch\" \"yarn redis\" \"yarn server2\" ",
      "typeorm": "TS_NODE=true ts-node ./node_modules/typeorm/cli.js"
   },
   "dependencies": {
      "apollo-server-express": "^2.19.0",
      "argon2": "^0.27.0",
      "class-validator": "^0.12.2",
      "connect-redis": "^5.0.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-session": "^1.17.1",
      "graphql": "^15.4.0",
      "passport": "^0.4.1",
      "passport-facebook": "^3.0.0",
      "pg": "^8.5.1",
      "redis": "^3.0.2",
      "reflect-metadata": "^0.1.10",
      "type-graphql": "^1.1.1",
      "typeorm": "0.2.29"
   },
   "devDependencies": {
      "@types/connect-redis": "^0.0.15",
      "@types/express": "^4.17.9",
      "@types/express-session": "^1.17.3",
      "@types/node": "^14.14.10",
      "@types/passport": "^1.0.5",
      "@types/passport-facebook": "^2.1.10",
      "@types/redis": "^2.8.28",
      "concurrently": "^5.3.0",
      "nodemon": "^2.0.6",
      "npm-force-resolutions": "^0.0.3",
      "ts-node": "9.0.0",
      "typescript": "4.1.2"
   },
