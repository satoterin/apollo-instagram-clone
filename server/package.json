{
   "name": "server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "main": "dist/index.js",
   "scripts": {
      "start": "node dist/index.js",
      "watch": "npx tsc -w",
      "server": "nodemon dist/index.js",
      "server2": "TS_NODE=true nodemon --exec ts-node src/index.ts",
      "redis": "redis-server",
      "dev": "concurrently \"yarn watch\" \"yarn redis\" \"yarn server\" ",
      "dev2": "concurrently \"yarn watch\" \"yarn redis\" \"yarn server2\" ",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js",
      "seed": "npx tsc && ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
      "heroku-postbuild": "cd ../../client && yarn build"
   },
   "dependencies": {
      "apollo-server-express": "^2.21.1",
      "argon2": "^0.27.1",
      "class-validator": "^0.13.1",
      "colors": "^1.4.0",
      "connect-mongo": "^4.4.0",
      "connect-redis": "^5.1.0",
      "dataloader": "^2.0.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-session": "^1.17.1",
      "graphql": "^15.5.0",
      "graphql-upload": "^11.0.0",
      "imagemin": "^7.0.1",
      "imagemin-mozjpeg": "^9.0.0",
      "is-jpg": "^2.0.0",
      "passport": "^0.4.1",
      "passport-facebook": "^3.0.0",
      "pg": "^8.5.1",
      "redis": "^3.0.2",
      "reflect-metadata": "^0.1.10",
      "sharp": "^0.27.2",
      "type-graphql": "^1.1.1",
      "typeorm": "0.2.31",
      "typeorm-seeding": "^1.6.1"
   },
   "devDependencies": {
      "@types/connect-mongo": "^3.1.3",
      "@types/connect-redis": "^0.0.16",
      "@types/express": "^4.17.11",
      "@types/express-session": "^1.17.3",
      "@types/graphql-upload": "^8.0.4",
      "@types/node": "^14.14.35",
      "@types/passport": "^1.0.6",
      "@types/passport-facebook": "^2.1.10",
      "@types/redis": "^2.8.28",
      "concurrently": "^6.0.0",
      "nodemon": "^2.0.7",
      "npm-force-resolutions": "^0.0.10",
      "ts-node": "9.1.1",
      "typescript": "4.2.3"
   },
   "license": "MIT",
   "resolutions": {
      "fs-capacitor": "^6.2.0",
      "graphql-upload": "^11.0.0"
   }
}
